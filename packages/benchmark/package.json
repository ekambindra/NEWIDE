{
  "name": "@ide/benchmark",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    }
  },
  "files": ["dist", "src", "README.md", "tasks", "fixtures"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "lint": "tsc -p tsconfig.json --noEmit",
    "corpus": "node dist/cli.js corpus --out tasks/default-corpus.json",
    "simulate": "node dist/cli.js simulate --seed 1337 --out fixtures/latest-results.json",
    "score:fixtures": "node dist/cli.js score fixtures/latest-results.json --history fixtures/metrics-history.jsonl --run-id fixture --out fixtures/latest-report.json",
    "gate:fixtures": "node dist/cli.js gate fixtures/latest-report.json"
  },
  "dependencies": {
    "@ide/shared": "0.1.0"
  },
  "bin": {
    "ide-benchmark": "dist/cli.js"
  }
}
